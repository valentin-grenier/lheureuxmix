		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Base",
					"slug": "base",
					"shadow": "0 10px 50px #00000010"
				},
				{
					"name": "Soft",
					"slug": "soft",
					"shadow": "0 10px 25px #00000010"
				}
			]
		},
		"custom": {
			"radius-sm": "0.5rem",
			"radius-md": "1rem",
			"radius-lg": "1.5rem",
			"radius-xl": "2.5rem",
			"color-red": "#b60000",
			"color-red-light": "#f8a9a950",
			"color-green": "#46b450",
			"color-green-light": "#46b44f77"
		}
	},
	"styles": {
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mulish)",
					"lineHeight": "normal"
				},
				"spacing": {
					"padding": {
						"top": "0.75rem",
						"bottom": "0.75rem"
					}
				},
				"border": {
					"radius": "var:custom|radius-sm"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mulish)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--p-22-pooperblackpro)",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mulish)",
					"fontWeight": "bold"
				},
				"css": "text-wrap: pretty;"
			},
			"link": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mulish)"
				}
			}
		},
		"blocks": {
			"core/cover": {
				"color": {
					"background": "var:preset|color|overlay-base"
				}
			},
			"core/button": {
				"typography": {
					"fontSize": "var:preset|font-size|sm"
				}
			},
			"core/social-link": {
				"color": {
					"text": "var:preset|color|secondary"
				},
				"typography": {
					"fontSize": "3rem"
				}
			},
			"core/embed": {
				"border": {
					"radius": "var:custom|radius-md"
				},
				"css": "overflow: hidden;"
			},
			"core/list-item": {
				"elements": {
					"link": {
						"color": { "text": "var:preset|color|secondary" },
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			}
		},
		"color": {
			"text": "var:preset|color|text"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--mulish)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "2rem"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json"
}
